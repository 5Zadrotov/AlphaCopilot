# OpenRouter API Key (get from https://openrouter.ai/keys)
OpenRouter__ApiKey=your_api_key_here

# JWT Configuration
Jwt__SecretKey=your_jwt_secret_key_here
Jwt__TokenExpirationHours=1
Jwt__RefreshTokenDays=14

# Database
ConnectionStrings__DefaultConnection=Host=postgres_server;Port=5432;Database=alphacopilot;Username=admin;Password=admin

# Logging
Logging__LogLevel__Default=Information
Logging__LogLevel__Microsoft.AspNetCore=Warning
